{
  "name": "Vision Automation",
  "slug": "vision-automation",
  "version": "1.0.0",
  "description": "Screen reading, object detection, and automated clicking",
  "author": "Aly-Joseph",
  "license": "MIT",
  "keywords": ["vision", "ocr", "screen", "automation", "click"],
